{
    "name": "Speedify",
    "description": "Speed-up or slow-down video playback",
    "version": "1.0",
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "run_at": "document_start",
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_title": "Speedify",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon.png"
    },
    "manifest_version": 2
}